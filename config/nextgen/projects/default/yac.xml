<?xml version="1.0" encoding="utf-8"?>
<!-- yet another config -->
<generator>
	<file name="bean" fileType="cfc" transformer="bean.xsl" directory="bean">
		<include file="bean/setMemento.xsl" />
		<include file="bean/validate.xsl" />
		<include file="bean/getset.xsl" />
		<include file="bean/dump.xsl" />
	</file>
	<file name="dao" fileNameAppend="DAO" filetype="cfc" transformer="dao.xsl" directory="dao">
		<include file="dao/create.xsl" />
		<include file="dao/read.xsl" />
		<include file="dao/update.xsl" />
		<include file="dao/delete.xsl" />
		<include file="dao/exists.xsl" />
		<include file="dao/save.xsl" />
		<include file="dao/queryRowToStruct.xsl" />
	</file>
	<file name="gateway" filetype="cfc" transformer="gateway.xsl" directory="gateway">
		<include file="gateway/getByAttributesQuery.xsl" />
		<include file="gateway/getByAttributes.xsl" />
		<include file="gateway/queryRowToStruct.xsl" />
	</file>
	<file name="service" filetype="cfc" transformer="service.xsl" directory="service">
		<include file="service/createBean.xsl" />
		<include file="service/getBean.xsl" />
		<include file="service/getBeans.xsl" />
		<include file="service/saveBean.xsl" />
		<include file="service/deleteBean.xsl" />
	</file>
	<file name="to" filetype="cfc" transformer="to.xsl" directory="to" />
	<file name="coldspring" filetype="xml" transformer="coldspring.xsl" directory="" fileNamePrepend="ColdSpring_"  />
	<file name="VO" filetype="as" transformer="VO.xsl" directory="VO" />
</generator>